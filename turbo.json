{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "compile": {
      "dependsOn": [
        "^compile"
      ],
      "outputs": [
        "lib/**",
        "dist/**",
        "types/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        "types/**"
      ]
    },
    "watch": {
      "dependsOn": [
        "^watch"
      ],
      "outputs": [
        "lib/**",
        "dist/**"
      ],
      "cache": false
    },
    "prepare-setup": {
      "dependsOn": [
        "^prepare-setup"
      ],
      "outputs": [
        "lib/**",
        "dist/**",
        "types/**"
      ]
    }
  }
}
